{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "SECRET_KEY": "@secret_key",
    "CLERK_SECRET_KEY": "@clerk_secret_key",
    "CLERK_JWT_PUBLIC_KEY": "@clerk_jwt_public_key"
  }
}
